<?xml version="1.0"?>
<cloudbees-web-app xmlns="http://www.cloudbees.com/xml/webapp/1">
  <resource name="jdbc/censord" auth="Container" type="javax.sql.DataSource">
    <param name="username" value="${db.username}" />
    <param name="password" value="${db.password}" />
    <param name="url" value="jdbc:cloudbees://${db.cloudBeesName}" />
  </resource>
</cloudbees-web-app>
